<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="354.0" prefWidth="498.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HotkeyInjector">
   <children>
      <TextArea fx:id="packetTxt" layoutX="35.0" layoutY="47.0" prefHeight="100.0" prefWidth="250.0" />
      <Label fx:id="packetValidLbl" layoutX="35.0" layoutY="30.0" text="Type your packets:" />
      <Button fx:id="addBtn" disable="true" layoutX="300.0" layoutY="47.0" mnemonicParsing="false" onAction="#onAddBtnClick" text="Add" />
      <ListView fx:id="packetList" layoutX="20.0" layoutY="174.0" prefHeight="162.0" prefWidth="200.0" />
      <TextField fx:id="keyTF" editable="false" layoutX="300.0" layoutY="97.0" prefHeight="25.0" prefWidth="52.0" />
      <Label layoutX="300.0" layoutY="80.0" text="Key" />
      <ChoiceBox fx:id="packetSideCb" layoutX="381.0" layoutY="97.0" prefHeight="25.0" prefWidth="96.0" />
      <Label layoutX="381.0" layoutY="80.0" text="Side" />
      <Button fx:id="removeEntryBtn" layoutX="226.0" layoutY="185.0" mnemonicParsing="false" onAction="#onRemoveBtnClick" text="Remove" />
      <Label layoutX="20.0" layoutY="157.0" text="List" />
