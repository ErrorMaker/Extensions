<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="117.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PosterMover">
   <children>
      <Button fx:id="upBtn" layoutX="75.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickUp" text="Up" />
      <Button fx:id="downBtn" layoutX="68.0" layoutY="80.0" mnemonicParsing="false" onAction="#onClickDown" text="Down" />
      <Button fx:id="leftBtn" layoutX="31.0" layoutY="54.0" mnemonicParsing="false" onAction="#onClickLeft" text="Left" />
      <Button fx:id="rightBtn" layoutX="114.0" layoutY="54.0" mnemonicParsing="false" onAction="#onClickRight" text="Right" />
      <Button fx:id="altUpBtn" layoutX="311.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickAltUp" text="Pull" />
      <Button fx:id="altDownBtn" layoutX="308.0" layoutY="80.0" mnemonicParsing="false" onAction="#onClickAltDown" text="Push" />
      <Button fx:id="altLeftBtn" layoutX="248.0" layoutY="54.0" mnemonicParsing="false" onAction="#onClickAltLeft" text="Wall left" />
      <Button fx:id="altRightBtn" layoutX="350.0" layoutY="54.0" mnemonicParsing="false" onAction="#onClickAltRight" text="Wall right" />
      <Button fx:id="rotateBtn" layoutX="173.0" layoutY="26.0" mnemonicParsing="false" onAction="#onClickRotate" text="Rotate" />
   </children>
</AnchorPane>
